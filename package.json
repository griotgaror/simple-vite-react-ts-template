{
    "name": "simple-vite-react-ts-template",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "dev": " vite --config vite.config.ts",
        "build": "vite build --config vite.config.ts",
        "testApi": "./node_modules/nodemon/bin/nodemon.js testApi/server.js",
        "lint:check": "npx eslint ./src",
        "lint:fix": "npx eslint ./src --fix",
        "prettier:check": "npx prettier ./src --check",
        "prettier:fix": "npx prettier ./src --write",
        "format": "npm run prettier:fix && npm run lint:fix",
        "test": "vitest --run",
        "test:ui": "vitest --ui",
        "updateVersion": "source nextVersion.sh --auto-commit",
        "submodule-update": "git submodule update --remote",
        "prepare": "husky"
    },
    "dependencies": {
        "@types/jest": "^29.5.14",
        "axios": "^1.3.4",
        "framer-motion": "^10.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-intersection-observer": "^9.13.1",
        "react-responsive": "^9.0.2",
        "react-router-dom": "^6.10.0",
        "styled-components": "^5.3.6",
        "vite": "^5.4.8",
        "vite-plugin-html": "^3.2.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.5.0",
        "@commitlint/config-conventional": "^19.5.0",
        "@testing-library/jest-dom": "^6.4.2",
        "@testing-library/react": "^14.2.2",
        "@types/cors": "^2.8.17",
        "@types/node": "^20.14.12",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "@types/styled-components": "^5.1.26",
        "@typescript-eslint/eslint-plugin": "^5.52.0",
        "@typescript-eslint/parser": "^5.52.0",
        "@vitejs/plugin-react": "^4.3.2",
        "@vitest/ui": "^2.1.2",
        "cors": "^2.8.5",
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "express": "^4.18.2",
        "husky": "^9.1.6",
        "jsdom": "^25.0.1",
        "nodemon": "^3.0.1",
        "prettier": "3.1.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.0.0",
        "vite-plugin-checker": "^0.8.0",
        "vitest": "^2.1.2"
    }
}
